<odoo>
    <template id="report_candidates">
        <t t-foreach="docs" t-as="candidate">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h2>
                            <span t-field="candidate.name"/>
                        </h2>
                        <div>
                            <strong>Expected Price: </strong>
                            <span t-field="candidate.age"/>
                        </div>
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Price</th>
                                </tr>
                            </thead>
                            <tbody>
                                <t t-set="skills" t-value="candidate.mapped('skill_ids')"/>
                                <tr t-foreach="skills" t-as="skill">
                                    <td>
                                        <span t-field="skill.name"/>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>